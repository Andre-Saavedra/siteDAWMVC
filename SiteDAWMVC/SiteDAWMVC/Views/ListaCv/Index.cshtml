@model IEnumerable<DadosPessoais>

@{
    ViewData["Title"] = "Index";
}

<h1>Curriculums</h1>

@if (Model.Count() > 0)
{
    <ul class="list-group list-group-flush">
        @foreach (var obj in Model)
        {
        <li class="list-group-item">@obj.Nome</li>
        }
    </ul>
}
else
{
    <p>Não existe nenhum curriculum para vizualizar</p>
}

