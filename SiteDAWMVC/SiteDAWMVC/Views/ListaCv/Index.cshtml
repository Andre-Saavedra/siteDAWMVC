@model IEnumerable<DadosPessoais>

@{
    ViewData["Title"] = "Index";
}

<h1>Curriculums</h1>

@if (Model.Count() > 0)
{
    <ul>
        @foreach (var obj in Model)
        {
            <li class="listaCv">
                <a>@obj.Nome</a>
            </li>
        }
    </ul>
}
else
{
    <p>Não existe nenhum curriculum para vizualizar</p>
}

